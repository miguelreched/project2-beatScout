<div class="grid-container">
  <h2>Followed users</h2>

  {{#each this.followedUser.followed}}
    <div>

      <img src="{{this.profilePic}}" alt="Profile picture" height="300px" />

      <p>{{this.username}}</p>
      <p>Country: {{this.country}}</p>

      <button>
        <a href="/all-users/{{this._id}}"> See more</a>
      </button>
    </div>
  {{/each}}

</div>