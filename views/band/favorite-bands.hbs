<div>
  <h3>Favorite Bands</h3>

  {{#each this.followedBand.favoriteBand}}
    <div>

    <img src="{{this.bandPic}}" alt="Profile picture"  height="300px">

      <p>{{this.name}}</p>
      <p>Genre: {{this.genre}}</p>
       <p>Info: {{this.info}}</p>


    <button>
    <a href="/band-info/{{this._id}}"> See more</a>
</button>



    </div>
  {{/each}}


</div>